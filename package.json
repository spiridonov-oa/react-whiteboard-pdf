{
  "name": "react-whiteboard-pdf",
  "version": "1.2.14",
  "description": "React whiteboard with PDF and Images import",
  "author": "Oleg Spiridonov <spiridonov.oa@gmail.com>",
  "private": false,
  "license": "MIT",
  "sideEffects": false,
  "main": "dist/index.js",
  "module": "dist/index.js",
  "keywords": [
    "react",
    "react-whiteboard",
    "whiteboard",
    "fabricjs",
    "pdf",
    "images",
    "draw",
    "paint",
    "JSON",
    "sketch"
  ],
  "files": [
    "dist",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/spiridonov-oa/react-whiteboard-pdf.git"
  },
  "dependencies": {
    "fabric": "^5.3.0",
    "file-saver": "^2.0.5",
    "prop-types": "^15.8.1",
    "react-pdf": "^5.3.2",
    "styled-components": "^5.3.11"
  },
  "peerDependencies": {
    "react": ">=17"
  },
  "devDependencies": {
    "@babel/cli": "^7.16.7",
    "@babel/core": "^7.16.7",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.18.2",
    "@babel/preset-react": "^7.17.12",
    "eslint": "^7.20.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-react": "*",
    "react": "*",
    "react-dom": "*",
    "react-scripts": "*"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "build-npm": "rm -rf dist && NODE_ENV=production babel src/lib --out-dir dist --copy-files",
    "update-version": "npm version ${npm_config_version:-patch} && npm i && npm run build && npm run build-npm && git add . && git commit -m 'Update build' && git push && npm publish",
    "test": "react-scripts test"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
