{
  "name": "react-whiteboard-pdf",
  "version": "1.2.17",
  "description": "React whiteboard with PDF and Images import",
  "author": "Oleg Spiridonov <spiridonov.oa@gmail.com>",
  "private": false,
  "license": "MIT",
  "sideEffects": false,
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "keywords": [
    "react",
    "react-whiteboard",
    "whiteboard",
    "fabric",
    "pdf",
    "images",
    "draw",
    "paint",
    "JSON",
    "sketch"
  ],
  "files": [
    "dist",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/spiridonov-oa/react-whiteboard-pdf.git"
  },
  "dependencies": {
    "core-js": "^3.30.1",
    "fabric": ">=6.6.2",
    "file-saver": ">=2.0.5",
    "react-pdf": ">=9.2.1",
    "regenerator-runtime": "^0.14.0",
    "styled-components": ">=5.3.11"
  },
  "peerDependencies": {
    "react": ">=18",
    "react-dom": ">=18"
  },
  "devDependencies": {
    "@types/file-saver": "^2.0.7",
    "@types/react": "^19.1.1",
    "@types/react-dom": "^19.1.2",
    "@types/styled-components": "^5.1.34",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-react": ">=7.33.2",
    "react": ">=18.2.0",
    "react-dom": ">=18.2.0",
    "react-scripts": ">=5.0.1",
    "ts-loader": "^9.5.2",
    "typescript": "~4.9.5"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "dev": "react-scripts start",
    "prepublishOnly": "npm run build-npm",
    "build-npm": "rm -rf dist && NODE_ENV=production babel src/lib --out-dir dist --copy-files --presets=@babel/preset-env && NODE_ENV=production BABEL_ENV=esm babel src/lib --out-dir dist/esm --copy-files && mv dist/esm/index.js dist/index.esm.js && rm -rf dist/esm",
    "update-version": "npm version ${npm_config_version:-patch} && npm i && npm run build && npm run build-npm && git add . && git commit -m 'Update build' && git push && npm publish",
    "test": "react-scripts test"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "exports": {
    ".": {
      "import": "./dist/index.esm.js",
      "require": "./dist/index.js"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
